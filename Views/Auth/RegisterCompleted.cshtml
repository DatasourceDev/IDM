@model IDM.DTO.BaseDTO

@{
    ViewData["Title"] = "ลงทะเบียน ผู้ใช้ชั่วคราวสำเร็จ";
    Layout = "_LayoutBox";

}
<div class="container">
    <div class="row vh-100 ">
        <div class="col-12 align-self-center">
            <div class="auth-page">
                <div class="card auth-card shadow-lg">
                    <div class="card-body">
                        <div class="px-3">
                            <div class="text-center auth-logo-text">
                                <h4 class="mt-0 mb-3 mt-5">ลงทะเบียนผู้ใช้ชั่วคราวสำเร็จ</h4>
                                <p class="text-muted mb-0">@LanguageLabel.GetLabel(Model.Languages, "Penging for Approve")</p>
                            </div> <!--end auth-logo-text-->
                        </div><!--end /div-->

                        <div class="m-3 text-center text-muted">
                            <a href="@Url.Action("Login","Auth", new { lang = Model.LanguageCode })" class="btn btn-gradient-info waves-effect waves-light" type="button"><i class="fas fa-sign-in-alt ml-1"></i> @LanguageLabel.GetLabel(Model.Languages, "Back")</a>
                        </div>
                    </div><!--end card-body-->
                </div><!--end card-->
            </div><!--end auth-page-->
        </div><!--end col-->
    </div><!--end row-->
</div>


